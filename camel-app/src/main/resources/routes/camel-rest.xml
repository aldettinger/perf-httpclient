<rest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://camel.apache.org/schema/spring"
	xsi:schemaLocation="http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring-4.2.0.xsd" path="/"
	description="REST API for SES"
	>

    <get path="/vertx" description="Vertx HTTP client Endpoint">
        <to uri="direct:vertx" />
    </get>

    <get path="/http" description="Apache HTTP client Endpoint">
        <to uri="direct:http" />
    </get>

    <get path="/netty" description="Netty HTTP client Endpoint">
        <to uri="direct:netty" />
    </get>
</rest>
